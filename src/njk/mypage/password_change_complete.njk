{% extends '_modules/_base.njk' %}
{%- set fileName = 'password_change_complete' -%}
{%- set pageName = 'password_change_complete' -%}
{%- set pageLevel = '2' -%}
{%- set pageTitle = 'パスワード編集' -%}
{%- set en_pageTitle = 'COMPLETE' -%}
{%- set pageDescription = 'エムステージ病院事務職求職支援のログインパスワード編集ページ。エムステージ病院事務職求職支援は、病院事務職、医療事務職に特化した転職・求人情報サイトです。医療経営に精通したコンサルタントが、あなたの転職活動をフルサポートします。' -%}
{%- set breadcrumbs = [{item: 'パスワード変更完了'}]-%}

{% block content %}

  <div class="container">
    <div class="passwordChangeComplete">
      <p class="passwordChangeComplete__guide">パスワード変更が完了いたしました。</p>
      <a href="{{ basePath }}login/" class="passwordChangeComplete__button is-hidden js-loginLink-target">ログインへ</a>
      <a href="{{ basePath }}mypage/" class="passwordChangeComplete__button is-hidden js-mypageLink-target">マイページへ</a>
    </div>
  </div>

{% endblock %}
